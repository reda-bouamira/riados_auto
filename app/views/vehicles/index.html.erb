<h1>Vehicles</h1>
<p>Find me in app/views/vehicles/index.html.erb</p>

<h1><%= @vehicles.first.car.make.name %></h1>



<div class=".container-fluid">
  <div class="row fluid">
    <div class="col pl-5 ml-5">
      <ul class="list-group list-group-flush">
        <% @vehicles.each do |vehicle| %>
          <li class="list-group-item"><%= " VIN: " + vehicle.vin  %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>